{
  "app": {
    "fontSize": {
      "surface": {
        "display": {
          "sm": {
            "$value": "3rem",
            "$type": "fontSizes"
          },
          "md": {
            "$value": "4rem",
            "$type": "fontSizes"
          },
          "lg": {
            "$value": "5rem",
            "$type": "fontSizes"
          },
          "xl": {
            "$value": "6rem",
            "$type": "fontSizes"
          }
        },
        "title": {
          "sm": {
            "$value": "1.25rem",
            "$type": "fontSizes"
          },
          "md": {
            "$value": "1.5rem",
            "$type": "fontSizes"
          },
          "lg": {
            "$value": "1.75rem",
            "$type": "fontSizes"
          },
          "xl": {
            "$value": "2rem",
            "$type": "fontSizes"
          }
        },
        "subtitle": {
          "sm": {
            "$value": "1.125rem",
            "$type": "fontSizes"
          },
          "md": {
            "$value": "1.375rem",
            "$type": "fontSizes"
          },
          "lg": {
