{
  "component": {
    "checkbox": {
      "color": {
        "unchecked": {
          "bg": {
            "default": {
              "value": "{app.color.component.focus.bg.default}",
              "type": "color"
            },
            "active": {
              "value": "{app.color.component.focus.bg.default}",
              "type": "color",
              "$extensions": {}
            }
          },
          "border": {
            "default": {
              "value": "{app.color.component.unchecked.border.translucent}",
              "type": "color"
            },
            "active": {
              "value": "{app.color.component.primary.bg.solid}",
              "type": "color",
              "$extensions": {
                "studio.tokens": {
                  "modify": {
                    "type": "darken",
                    "value": "{config.color.primary.darken.active}",
                    "space": "hsl"
                  }
                }
              }
            }
          },
          "fg": {
            "default": {
              "value": "{app.color.component.primary.fg.solid}",
              "type": "color"
            },
            "active": {
              "value": "{app.color.component.primary.bg.solid}",
              "type": "color",
              "$extensions": {
                "studio.tokens": {
                  "modify": {
                    "type": "darken",
                    "value": "{config.color.primary.darken.active}",
                    "space": "hsl"
                  }
                }
              }
            }
          }
        },
        "checked": {
          "bg": {
            "default": {
              "value": "{app.color.component.primary.bg.solid}",
              "type": "color"
            },
            "active": {
              "value": "{app.color.component.primary.bg.solid}",
              "type": "color",
              "$extensions": {
                "studio.tokens": {
                  "modify": {
                    "type": "darken",
                    "value": "{config.color.primary.darken.active}",
                    "space": "hsl"
                  }
                }
              }
            }
          },
          "border": {
            "default": {
              "value": "transparent",
              "type": "color"
            },
            "active": {
              "value": "transparent",
              "type": "color",
              "$extensions": {}
            }
          },
          "fg": {
            "default": {
              "value": "{app.color.component.on.fg.solid}",
              "type": "color"
            },
            "active": {
              "value": "{app.color.component.on.fg.solid}",
              "type": "color",
              "$extensions": {}
            }
          }
        },
        "disabled": {
          "bg": {
            "default": {
              "value": "{app.color.component.disabled.bg.translucent}",
              "type": "color"
            }
          },
          "fg": {
            "default": {
              "value": "{app.color.component.disabled.fg.onTranslucent}",
              "type": "color"
            }
          },
          "border": {
            "default": {
              "value": "{app.color.component.disabled.border.translucent}",
              "type": "color"
            }
          }
        }
      },
      "fontSize": {
        "icon": {
          "sm": {
            "value": "{component.checkbox.dimension.mark.height.sm}",
            "type": "fontSizes"
          },
          "md": {
            "value": "{component.checkbox.dimension.mark.height.md}",
            "type": "fontSizes"
          },
          "lg": {
            "value": "{component.checkbox.dimension.mark.height.lg}",
            "type": "fontSizes"
          }
        }
      },
      "dimension": {
        "box": {
          "height": {
            "sm": {
              "value": "{component.checkbox.dimension.box.height.md} - {component.checkbox.config.box.height.step}",
              "type": "dimension"
            },
            "md": {
              "value": "{component.checkbox.config.box.height.default}",
              "type": "dimension"
            },
            "lg": {
              "value": "{component.checkbox.dimension.box.height.md} + {component.checkbox.config.box.height.step}",
              "type": "dimension"
            }
          },
          "width": {
            "sm": {
              "value": "{component.checkbox.dimension.box.height.sm} * {component.checkbox.config.box.width.ratio}",
              "type": "dimension"
            },
            "md": {
              "value": "{component.checkbox.dimension.box.height.md} * {component.checkbox.config.box.width.ratio}",
              "type": "dimension"
            },
            "lg": {
              "value": "{component.checkbox.dimension.box.height.lg} * {component.checkbox.config.box.width.ratio}",
              "type": "dimension"
            }
          },
          "radius": {
            "sm": {
              "value": "{component.checkbox.dimension.box.radius.md} - {component.checkbox.config.box.radius.step}",
              "type": "dimension"
            },
            "md": {
              "value": "{component.checkbox.config.box.radius.default}",
              "type": "dimension"
            },
            "lg": {
              "value": "{component.checkbox.dimension.box.radius.md} + {component.checkbox.config.box.radius.step}",
              "type": "dimension"
            }
          },
          "border": {
            "sm": {
              "value": "{component.checkbox.dimension.box.border.md} - {component.checkbox.config.box.border.step}",
              "type": "dimension"
            },
            "md": {
              "value": "{component.checkbox.config.box.border.default}",
              "type": "dimension"
            },
            "lg": {
              "value": "{component.checkbox.dimension.box.border.md} + {component.checkbox.config.box.border.step}",
              "type": "dimension"
            }
          }
        },
        "mark": {
          "height": {
            "sm": {
              "value": "{component.checkbox.dimension.box.height.sm} * {component.checkbox.config.mark.scale}",
              "type": "dimension"
            },
            "md": {
              "value": "{component.checkbox.dimension.box.height.md} * {component.checkbox.config.mark.scale}",
              "type": "dimension"
            },
            "lg": {
              "value": "{component.checkbox.dimension.box.height.lg} * {component.checkbox.config.mark.scale}",
              "type": "dimension"
            }
          },
          "width": {
            "sm": {
              "value": "{component.checkbox.dimension.mark.height.sm} * {component.checkbox.config.box.width.ratio}",
              "type": "dimension"
            },
            "md": {
              "value": "{component.checkbox.dimension.mark.height.md} * {component.checkbox.config.box.width.ratio}",
              "type": "dimension"
            },
            "lg": {
              "value": "{component.checkbox.dimension.mark.height.lg} * {component.checkbox.config.box.width.ratio}",
              "type": "dimension"
            }
          }
        }
      },
      "config": {
        "box": {
          "height": {
            "default": {
              "value": "{app.dimension.control.height.md} / 2",
              "type": "other"
            },
            "step": {
              "value": "0.5rem",
              "type": "other"
            }
          },
          "radius": {
            "default": {
              "value": "0.5rem",
              "type": "other"
            },
            "step": {
              "value": "0.125rem",
              "type": "other"
            }
          },
          "width": {
            "ratio": {
              "value": "1",
              "type": "other"
            }
          },
          "border": {
            "default": {
              "value": "0.125rem",
              "type": "other"
            },
            "step": {
              "value": "0rem",
              "type": "other"
            }
          }
        },
        "mark": {
          "scale": {
            "value": "0.75",
            "type": "other"
          }
        }
      },
      "typography": {
        "default": {
          "value": "{component.button.typography.symbol}",
          "type": "typography"
        }
      }
    }
  }
}