{
  "component": {
    "menuBox": {
      "dimension": {
        "padding": {
          "x": {
            "value": "0.25rem",
            "type": "dimension"
          },
          "y": {
            "value": "0.25rem",
            "type": "dimension"
          }
        },
        "gap": {
          "default": {
            "value": "0rem",
            "type": "dimension"
          }
        },
        "blur": {
          "default": {
            "value": "8rem",
            "type": "dimension"
          }
        },
        "radius": {
          "xs": {
            "value": "{app.dimension.control.radius.xs} + {component.menuBox.dimension.padding.x}",
            "type": "dimension"
          },
          "sm": {
            "value": "{app.dimension.control.radius.sm} + {component.menuBox.dimension.padding.x}",
            "type": "dimension"
          },
          "md": {
            "value": "{app.dimension.control.radius.md} + {component.menuBox.dimension.padding.x}",
            "type": "dimension"
          },
          "lg": {
            "value": "{app.dimension.control.radius.lg} + {component.menuBox.dimension.padding.x}",
            "type": "dimension"
          }
        },
        "border": {
          "default": {
            "value": "{app.dimension.control.border.default}",
            "type": "dimension"
          }
        }
      },
      "color": {
        "bg": {
          "value": "{app.color.surface.elevation.base.bg.default}",
          "type": "color",
          "$extensions": {
            "studio.tokens": {
              "modify": {
                "type": "alpha",
                "value": "0.8",
                "space": "hsl"
              }
            }
          }
        },
        "border": {
          "value": "transparent",
          "type": "color"
        }
      },
      "boxShadow": {
        "default": {
          "value": "{app.boxShadow.component.high.default}",
          "type": "boxShadow"
        }
      }
    }
  }
}