{
  "component": {
    "menuBox": {
      "dimension": {
        "padding": {
          "x": {
            "$value": "0.25rem",
            "$type": "dimension"
          },
          "y": {
            "$value": "0.25rem",
            "$type": "dimension"
          }
        },
        "gap": {
          "default": {
            "$value": "0rem",
            "$type": "dimension"
          }
        },
        "blur": {
          "default": {
            "$value": "8rem",
            "$type": "dimension"
          }
        },
        "radius": {
          "xs": {
            "$value": "{app.dimension.component.radius.xs} + {component.menuBox.dimension.padding.x}",
            "$type": "dimension"
          },
          "sm": {
            "$value": "{app.dimension.component.radius.sm} + {component.menuBox.dimension.padding.x}",
            "$type": "dimension"
          },
          "md": {
            "$value": "{app.dimension.component.radius.md} + {component.menuBox.dimension.padding.x}",
            "$type": "dimension"
          },
          "lg": {
            "$value": "{app.dimension.component.radius.lg} + {component.menuBox.dimension.padding.x}",
            "$type": "dimension"
          }
        },
        "border": {
          "default": {
            "$value": "{app.dimension.component.border.default}",
            "$type": "dimension"
          }
        }
      },
      "color": {
        "bg": {
          "$value": "{app.color.surface.elevation.high.bg}",
          "$type": "color"
        },
        "border": {
          "$value": "transparent",
          "$type": "color"
        }
      },
      "boxShadow": {
        "default": {
          "$value": "{app.boxShadow.component.high.default}",
          "$type": "boxShadow"
        }
      }
    }
  }
}
