{
  "component": {
    "menuBox": {
      "dimension": {
        "padding": {
          "x": {
            "$type": "dimension",
            "$value": "0.25rem"
          },
          "y": {
            "$type": "dimension",
            "$value": "0.25rem"
          }
        },
        "gap": {
          "default": {
            "$type": "dimension",
            "$value": "0rem"
          }
        },
        "blur": {
          "default": {
            "$type": "dimension",
            "$value": "8rem"
          }
        },
        "radius": {
          "xs": {
            "$type": "dimension",
            "$value": "{app.dimension.control.radius.xs} + {component.menuBox.dimension.padding.x}"
          },
          "sm": {
            "$type": "dimension",
            "$value": "{app.dimension.control.radius.sm} + {component.menuBox.dimension.padding.x}"
          },
          "md": {
            "$type": "dimension",
            "$value": "{app.dimension.control.radius.md} + {component.menuBox.dimension.padding.x}"
          },
          "lg": {
            "$type": "dimension",
            "$value": "{app.dimension.control.radius.lg} + {component.menuBox.dimension.padding.x}"
          }
        },
        "border": {
          "default": {
            "$type": "dimension",
            "$value": "{app.dimension.control.border.default}"
          }
        }
      },
      "color": {
        "bg": {
          "$type": "color",
          "$value": "{app.color.surface.elevation.high.bg}"
        },
        "border": {
          "$type": "color",
          "$value": "transparent"
        }
      },
      "boxShadow": {
        "default": {
          "$type": "boxShadow",
          "$value": "{app.boxShadow.component.high.default}"
        }
      }
    }
  }
}