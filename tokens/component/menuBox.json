{
  "component": {
    "menuBox": {
      "dimension": {
        "padding": {
          "x": {
            "$value": "{app.dimension.component.spacing.1}",
            "$type": "dimension"
          },
          "y": {
            "$value": "{app.dimension.component.spacing.1}",
            "$type": "dimension"
          }
        },
        "gap": {
          "default": {
            "$value": "0",
            "$type": "dimension"
          }
        },
        "blur": {
          "default": {
            "$value": "8rem",
            "$type": "dimension"
          }
        },
        "radius": {
          "xs": {
            "$value": "{app.dimension.component.radius.3} + {component.menuBox.dimension.padding.x}",
            "$type": "dimension"
          },
          "sm": {
            "$value": "{app.dimension.component.radius.3_5} + {component.menuBox.dimension.padding.x}",
            "$type": "dimension"
          },
          "md": {
            "$value": "{app.dimension.component.radius.4} + {component.menuBox.dimension.padding.x}",
            "$type": "dimension"
          },
          "lg": {
            "$value": "{app.dimension.component.radius.4_5} + {component.menuBox.dimension.padding.x}",
            "$type": "dimension"
          }
        },
        "border": {
          "default": {
            "$value": "{app.dimension.component.border.default}",
            "$type": "dimension"
          }
        }
      },
      "color": {
        "bg": {
          "$value": "{app.color.component.primary.bg.inverted.default}",
          "$type": "color"
        },
        "border": {
          "$value": "transparent",
          "$type": "color"
        }
      },
      "boxShadow": {
        "default": {
          "$value": "{app.boxShadow.component.high}",
          "$type": "boxShadow"
        }
      }
    }
  }
}
