{
  "component": {
    "input": {
      "fontFamily": {
        "default": {
          "$type": "fontFamilies",
          "$value": "{app.fontFamily.control.text}"
        },
        "symbol": {
          "$type": "fontFamilies",
          "$value": "{app.fontFamily.control.symbol}"
        }
      },
      "fontWeight": {
        "input": {
          "$type": "fontWeights",
          "$value": "{app.fontWeight.control.text.default}"
        },
        "label": {
          "$type": "fontWeights",
          "$value": "{app.fontWeight.label.text.default}"
        },
        "symbol": {
          "$type": "fontWeights",
          "$value": "{app.fontWeight.control.symbol.default}"
        }
      },
      "lineHeight": {
        "input": {
          "$type": "lineHeights",
          "$value": "{app.lineHeight.default}"
        },
        "label": {
          "$type": "lineHeights",
          "$value": "{app.lineHeight.none}"
        },
        "symbol": {
          "$type": "lineHeights",
          "$value": "{app.lineHeight.none}"
        },
        "leading": {
          "reset": {
            "$type": "lineHeights",
            "$value": "100%"
          }
        }
      },
      "fontSize": {
        "input": {
          "xs": {
            "$type": "fontSizes",
            "$value": "{component.input.fontSize.input.sm} - {component.input.config.text.step}"
          },
          "sm": {
            "$type": "fontSizes",
            "$value": "{component.input.fontSize.input.md} - {component.input.config.text.step}"
          },
          "md": {
            "$type": "fontSizes",
            "$value": "{app.fontSize.control.text.sm}"
          },
          "lg": {
            "$type": "fontSizes",
            "$value": "{component.input.fontSize.input.md} + {component.input.config.text.step}"
          }
        },
        "symbol": {
          "xs": {
            "$type": "fontSizes",
            "$value": "{component.input.fontSize.symbol.sm} - {component.input.config.symbol.step}"
          },
          "sm": {
            "$type": "fontSizes",
            "$value": "{component.input.fontSize.symbol.md} - {component.input.config.symbol.step}"
          },
          "md": {
            "$type": "fontSizes",
            "$value": "{app.fontSize.control.symbol.md}"
          },
          "lg": {
            "$type": "fontSizes",
            "$value": "{component.input.fontSize.symbol.md} + {component.input.config.symbol.step}"
          }
        },
        "label": {
          "xs": {
            "$type": "fontSizes",
            "$value": "{app.fontSize.label.text.xs}"
          },
          "sm": {
            "$type": "fontSizes",
            "$value": "{app.fontSize.label.text.sm}"
          },
          "md": {
            "$type": "fontSizes",
            "$value": "{app.fontSize.label.text.sm}"
          },
          "lg": {
            "$type": "fontSizes",
            "$value": "{app.fontSize.label.text.sm}"
          }
        }
      },
      "letterSpacing": {
        "default": {
          "$type": "letterSpacing",
          "$value": "{app.letterSpacing.control.text}"
        }
      },
      "textCase": {
        "default": {
          "$type": "textCase",
          "$value": "{app.textCase.control.default}"
        }
      },
      "color": {
        "default": {
          "bg": {
            "default": {
              "$type": "color",
              "$value": "transparent"
            }
          },
          "border": {
            "default": {
              "$type": "color",
              "$value": "{app.color.component.primary.border.translucent.default}"
            }
          },
          "fg": {
            "default": {
              "$type": "color",
              "$value": "{app.color.surface.fg.default}"
            },
            "muted": {
              "$type": "color",
              "$value": "{app.color.surface.fg.muted}"
            },
            "label": {
              "$type": "color",
              "$value": "{app.color.component.primary.fg.subtle.default}"
            }
          }
        },
        "focus": {
          "border": {
            "default": {
              "$type": "color",
              "$value": "{app.color.component.focus.border.default}"
            }
          },
          "fg": {
            "label": {
              "$type": "color",
              "$value": "{app.color.component.primary.fg.subtle.default}"
            }
          }
        },
        "danger": {
          "bg": {
            "default": {
              "$type": "color",
              "$value": "{app.color.component.danger.bg.translucent.default}"
            }
          },
          "border": {
            "default": {
              "$type": "color",
              "$value": "{app.color.component.danger.border.solid.default}"
            }
          },
          "fg": {
            "default": {
              "$type": "color",
              "$value": "{app.color.component.danger.fg.onSolid}"
            },
            "muted": {
              "$type": "color",
              "$value": "{app.color.component.danger.fg.solid.default}"
            },
            "label": {
              "$type": "color",
              "$value": "{app.color.component.danger.fg.muted}"
            }
          }
        },
        "disabled": {
          "bg": {
            "default": {
              "$type": "color",
              "$value": "transparent"
            }
          },
          "border": {
            "default": {
              "$type": "color",
              "$value": "{app.color.component.disabled.border.translucent}"
            }
          },
          "fg": {
            "default": {
              "$type": "color",
              "$value": "{app.color.component.disabled.fg.solid}"
            },
            "label": {
              "$type": "color",
              "$value": "{app.color.component.disabled.fg.solid}"
            }
          }
        }
      },
      "dimension": {
        "height": {
          "xs": {
            "$type": "dimension",
            "$value": "{app.dimension.control.height.xs}"
          },
          "sm": {
            "$type": "dimension",
            "$value": "{app.dimension.control.height.sm}"
          },
          "md": {
            "$type": "dimension",
            "$value": "{app.dimension.control.height.md}"
          },
          "lg": {
            "$type": "dimension",
            "$value": "{app.dimension.control.height.lg}"
          }
        },
        "padding": {
          "default": {
            "xs": {
              "$type": "dimension",
              "$value": "{app.dimension.control.padding.xs}"
            },
            "sm": {
              "$type": "dimension",
              "$value": "{app.dimension.control.padding.sm}"
            },
            "md": {
              "$type": "dimension",
              "$value": "{app.dimension.control.padding.md}"
            },
            "lg": {
              "$type": "dimension",
              "$value": "{app.dimension.control.padding.lg}"
            }
          },
          "label": {
            "xs": {
              "$type": "dimension",
              "$value": "4px"
            },
            "sm": {
              "$type": "dimension",
              "$value": "4px"
            },
            "md": {
              "$type": "dimension",
              "$value": "4px"
            },
            "lg": {
              "$type": "dimension",
              "$value": "4px"
            }
          },
          "slot": {
            "xs": {
              "$type": "dimension",
              "$value": "0.25rem"
            },
            "sm": {
              "$type": "dimension",
              "$value": "0.25rem"
            },
            "md": {
              "$type": "dimension",
              "$value": "0.25rem"
            },
            "lg": {
              "$type": "dimension",
              "$value": "0.25rem"
            }
          }
        },
        "gap": {
          "default": {
            "xs": {
              "$type": "dimension",
              "$value": "{component.input.dimension.gap.default.sm} - {component.input.config.gap.default.step}"
            },
            "sm": {
              "$type": "dimension",
              "$value": "{component.input.dimension.gap.default.md} - {component.input.config.gap.default.step}"
            },
            "md": {
              "$type": "dimension",
              "$value": "{app.dimension.control.gap.md}"
            },
            "lg": {
              "$type": "dimension",
              "$value": "{component.input.dimension.gap.default.md} + {component.input.config.gap.default.step}"
            }
          },
          "label": {
            "xs": {
              "$type": "dimension",
              "$value": "{component.input.dimension.gap.label.sm} - {component.input.config.gap.label.step}"
            },
            "sm": {
              "$type": "dimension",
              "$value": "{component.input.dimension.gap.label.md} - {component.input.config.gap.label.step}"
            },
            "md": {
              "$type": "dimension",
              "$value": "{app.dimension.control.gap.sm}"
            },
            "lg": {
              "$type": "dimension",
              "$value": "{component.input.dimension.gap.label.md} + {component.input.config.gap.label.step}"
            }
          }
        },
        "radius": {
          "xs": {
            "$type": "dimension",
            "$value": "{app.dimension.control.radius.xs}"
          },
          "sm": {
            "$type": "dimension",
            "$value": "{app.dimension.control.radius.sm}"
          },
          "md": {
            "$type": "dimension",
            "$value": "{app.dimension.control.radius.md}"
          },
          "lg": {
            "$type": "dimension",
            "$value": "{app.dimension.control.radius.lg}"
          }
        },
        "border": {
          "xs": {
            "$type": "dimension",
            "$value": "{app.dimension.control.border.default}"
          },
          "sm": {
            "$type": "dimension",
            "$value": "{app.dimension.control.border.default}"
          },
          "md": {
            "$type": "dimension",
            "$value": "{app.dimension.control.border.default}"
          },
          "lg": {
            "$type": "dimension",
            "$value": "{app.dimension.control.border.default}"
          }
        },
        "symbol": {
          "xs": {
            "$type": "dimension",
            "$value": "{component.input.fontSize.symbol.xs}"
          },
          "sm": {
            "$type": "dimension",
            "$value": "{component.input.fontSize.symbol.sm}"
          },
          "md": {
            "$type": "dimension",
            "$value": "{component.input.fontSize.symbol.md}"
          },
          "lg": {
            "$type": "dimension",
            "$value": "{component.input.fontSize.symbol.lg}"
          }
        },
        "blur": {
          "default": {
            "$type": "dimension",
            "$value": "1rem"
          }
        }
      },
      "config": {
        "symbol": {
          "step": {
            "$type": "other",
            "$value": "0.125rem"
          }
        },
        "text": {
          "step": {
            "$type": "other",
            "$value": "0.0625rem"
          }
        },
        "gap": {
          "default": {
            "step": {
              "$type": "other",
              "$value": "0.125rem"
            }
          },
          "label": {
            "step": {
              "$type": "other",
              "$value": "0.0625rem"
            }
          }
        }
      },
      "boxShadow": {
        "focusRing": {
          "$type": "boxShadow",
          "$value": "{app.boxShadow.focusRing.md}"
        }
      }
    }
  }
}