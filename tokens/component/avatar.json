{
  "component": {
    "avatar": {
      "color": {
        "solid": {
          "bg": {
            "default": {
              "$value": "{app.color.component.primary.bg.default}",
              "$type": "color"
            }
          },
          "border": {
            "default": {
              "$value": "transparent",
              "$type": "color"
            }
          },
          "fg": {
            "default": {
              "$value": "{app.color.component.primary.fg.onSolid.default}",
              "$type": "color"
            }
          }
        },
        "outline": {
          "bg": {
            "default": {
              "$value": "transparent",
              "$type": "color"
            }
          },
          "border": {
            "default": {
              "$value": "{app.color.component.primary.border.translucent.default}",
              "$type": "color"
            }
          },
          "fg": {
            "default": {
              "$value": "{app.color.component.primary.fg.translucent.default}",
              "$type": "color"
            }
          }
        }
      },
      "dimension": {
        "width": {
          "xs": {
            "default": {
              "$value": "{component.avatar.dimension.height.xs.default} * {component.avatar.config.default.sizing.ratio}",
              "$type": "dimension"
            },
            "inline": {
              "$value": "{component.avatar.dimension.width.xs.default} - {component.avatar.config.inline.sizing.step}",
              "$type": "dimension"
            }
          },
          "sm": {
            "default": {
              "$value": "{component.avatar.dimension.height.sm.default} * {component.avatar.config.default.sizing.ratio}",
              "$type": "dimension"
            },
            "inline": {
              "$value": "{component.avatar.dimension.width.sm.default} - {component.avatar.config.inline.sizing.step}",
              "$type": "dimension"
            }
          },
          "md": {
            "default": {
              "$value": "{component.avatar.dimension.height.md.default} * {component.avatar.config.default.sizing.ratio}",
              "$type": "dimension"
            },
            "inline": {
              "$value": "{component.avatar.dimension.width.md.default} - {component.avatar.config.inline.sizing.step}",
              "$type": "dimension"
            }
          },
          "lg": {
            "default": {
              "$value": "{component.avatar.dimension.height.lg.default} * {component.avatar.config.default.sizing.ratio}",
              "$type": "dimension"
            },
            "inline": {
              "$value": "{component.avatar.dimension.width.lg.default} - {component.avatar.config.inline.sizing.step}",
              "$type": "dimension"
            }
          },
          "xl": {
            "default": {
              "$value": "{component.avatar.dimension.height.xl.default} * {component.avatar.config.default.sizing.ratio}",
              "$type": "dimension"
            },
            "inline": {
              "$value": "{component.avatar.dimension.width.xl.default} - {component.avatar.config.inline.sizing.step}",
              "$type": "dimension"
            }
          }
        },
        "height": {
          "xs": {
            "default": {
              "$value": "{component.avatar.dimension.height.sm.default} - {component.avatar.config.default.sizing.step}",
              "$type": "dimension"
            },
            "inline": {
              "$value": "{component.avatar.dimension.height.xs.default} - {component.avatar.config.inline.sizing.step}",
              "$type": "dimension"
            }
          },
          "sm": {
            "default": {
              "$value": "{component.avatar.dimension.height.md.default} - {component.avatar.config.default.sizing.step}",
              "$type": "dimension"
            },
            "inline": {
              "$value": "{app.dimension.component.height.lg} - {component.avatar.config.inline.sizing.step}",
              "$type": "dimension"
            }
          },
          "md": {
            "default": {
              "$value": "{app.dimension.component.height.xl}",
              "$type": "dimension"
            },
            "inline": {
              "$value": "{component.avatar.dimension.height.md.default} - {component.avatar.config.inline.sizing.step}",
              "$type": "dimension"
            }
          },
          "lg": {
            "default": {
              "$value": "{component.avatar.dimension.height.md.default} + {component.avatar.config.default.sizing.step}",
              "$type": "dimension"
            },
            "inline": {
              "$value": "{component.avatar.dimension.height.lg.default} - {component.avatar.config.inline.sizing.step}",
              "$type": "dimension"
            }
          },
          "xl": {
            "default": {
              "$value": "{component.avatar.dimension.height.lg.default} + {component.avatar.config.default.sizing.step}",
              "$type": "dimension"
            },
            "inline": {
              "$value": "{component.avatar.dimension.height.xl.default} - {component.avatar.config.inline.sizing.step}",
              "$type": "dimension"
            }
          }
        },
        "radius": {
          "xs": {
            "default": {
              "$value": "{component.avatar.dimension.radius.sm.default} - {component.avatar.config.default.radius.step}",
              "$type": "dimension"
            },
            "inline": {
              "$value": "{component.avatar.dimension.radius.xs.default} - {component.avatar.config.inline.sizing.step} / 2",
              "$type": "dimension"
            }
          },
          "sm": {
            "default": {
              "$value": "{component.avatar.dimension.radius.md.default} - {component.avatar.config.default.radius.step}",
              "$type": "dimension"
            },
            "inline": {
              "$value": "{component.avatar.dimension.radius.sm.default} - {component.avatar.config.inline.sizing.step} / 2",
              "$type": "dimension"
            }
          },
          "md": {
            "default": {
              "$value": "1rem",
              "$type": "dimension"
            },
            "inline": {
              "$value": "{component.avatar.dimension.radius.md.default} - {component.avatar.config.inline.sizing.step} / 2",
              "$type": "dimension"
            }
          },
          "lg": {
            "default": {
              "$value": "{component.avatar.dimension.radius.md.default} + {component.avatar.config.default.radius.step}",
              "$type": "dimension"
            },
            "inline": {
              "$value": "{component.avatar.dimension.radius.lg.default} - {component.avatar.config.inline.sizing.step} / 2",
              "$type": "dimension"
            }
          },
          "xl": {
            "default": {
              "$value": "{component.avatar.dimension.radius.lg.default} + {component.avatar.config.default.radius.step}",
              "$type": "dimension"
            },
            "inline": {
              "$value": "{component.avatar.dimension.radius.xl.default} - {component.avatar.config.inline.sizing.step} / 2",
              "$type": "dimension"
            }
          }
        },
        "border": {
          "default": {
            "$value": "{app.dimension.component.border.default}",
            "$type": "dimension"
          }
        },
        "symbol": {
          "xs": {
            "default": {
              "$value": "{component.avatar.fontSize.xs.symbol.default}",
              "$type": "dimension"
            },
            "inline": {
              "$value": "{component.avatar.fontSize.xs.symbol.inline}",
              "$type": "dimension"
            }
          },
          "sm": {
            "default": {
              "$value": "{component.avatar.fontSize.sm.symbol.default}",
              "$type": "dimension"
            },
            "inline": {
              "$value": "{component.avatar.fontSize.sm.symbol.inline}",
              "$type": "dimension"
            }
          },
          "md": {
            "default": {
              "$value": "{component.avatar.fontSize.md.symbol.default}",
              "$type": "dimension"
            },
            "inline": {
              "$value": "{component.avatar.fontSize.md.symbol.inline}",
              "$type": "dimension"
            }
          },
          "lg": {
            "default": {
              "$value": "{component.avatar.fontSize.lg.symbol.default}",
              "$type": "dimension"
            },
            "inline": {
              "$value": "{component.avatar.fontSize.lg.symbol.inline}",
              "$type": "dimension"
            }
          },
          "xl": {
            "default": {
              "$value": "{component.avatar.fontSize.xl.symbol.default}",
              "$type": "dimension"
            },
            "inline": {
              "$value": "{component.avatar.fontSize.xl.symbol.inline}",
              "$type": "dimension"
            }
          }
        }
      },
      "fontFamily": {
        "default": {
          "$value": "{app.fontFamily.component.number}",
          "$type": "fontFamilies"
        },
        "icon": {
          "$value": "{app.fontFamily.component.symbol}",
          "$type": "fontFamilies"
        }
      },
      "fontWeight": {
        "default": {
          "$value": "{app.fontWeight.component.text.strong}",
          "$type": "fontWeights"
        }
      },
      "lineHeight": {
        "default": {
          "$value": "{app.lineHeight.none}",
          "$type": "lineHeights"
        }
      },
      "fontSize": {
        "xs": {
          "default": {
            "$value": "{component.avatar.fontSize.sm.default} - ({component.avatar.config.default.sizing.step} / 4)",
            "$type": "fontSizes"
          },
          "inline": {
            "$value": "{component.avatar.fontSize.xs.default} - {component.avatar.config.inline.sizing.fontSize}",
            "$type": "fontSizes"
          },
          "symbol": {
            "default": {
              "$value": "{component.avatar.fontSize.xs.default} * 1.5",
              "$type": "fontSizes"
            },
            "inline": {
              "$value": "{component.avatar.fontSize.xs.symbol.default} - {component.avatar.config.inline.sizing.fontSize}",
              "$type": "fontSizes"
            }
          }
        },
        "sm": {
          "default": {
            "$value": "{component.avatar.fontSize.md.default} - ({component.avatar.config.default.sizing.step} / 4)",
            "$type": "fontSizes"
          },
          "inline": {
            "$value": "{component.avatar.fontSize.sm.default} - {component.avatar.config.inline.sizing.fontSize}",
            "$type": "fontSizes"
          },
          "symbol": {
            "default": {
              "$value": "{component.avatar.fontSize.sm.default} * 1.5",
              "$type": "fontSizes"
            },
            "inline": {
              "$value": "{component.avatar.fontSize.sm.symbol.default} - {component.avatar.config.inline.sizing.fontSize}",
              "$type": "fontSizes"
            }
          }
        },
        "md": {
          "default": {
            "$value": "{app.fontSize.component.text.xl}",
            "$type": "fontSizes"
          },
          "inline": {
            "$value": "{component.avatar.fontSize.md.default} - {component.avatar.config.inline.sizing.fontSize}",
            "$type": "fontSizes"
          },
          "symbol": {
            "default": {
              "$value": "{component.avatar.fontSize.md.default} * 1.5",
              "$type": "fontSizes"
            },
            "inline": {
              "$value": "{component.avatar.fontSize.md.symbol.default} - {component.avatar.config.inline.sizing.fontSize}",
              "$type": "fontSizes"
            }
          }
        },
        "lg": {
          "default": {
            "$value": "{component.avatar.fontSize.md.default} + ({component.avatar.config.default.sizing.step} / 4)",
            "$type": "fontSizes"
          },
          "inline": {
            "$value": "{component.avatar.fontSize.lg.default} - {component.avatar.config.inline.sizing.fontSize}",
            "$type": "fontSizes"
          },
          "symbol": {
            "default": {
              "$value": "{component.avatar.fontSize.lg.default} * 1.5",
              "$type": "fontSizes"
            },
            "inline": {
              "$value": "{component.avatar.fontSize.lg.symbol.default} - {component.avatar.config.inline.sizing.fontSize}",
              "$type": "fontSizes"
            }
          }
        },
        "xl": {
          "default": {
            "$value": "{component.avatar.fontSize.lg.default} + ({component.avatar.config.default.sizing.step} / 4)",
            "$type": "fontSizes"
          },
          "inline": {
            "$value": "{component.avatar.fontSize.xl.default} - {component.avatar.config.inline.sizing.fontSize}",
            "$type": "fontSizes"
          },
          "symbol": {
            "default": {
              "$value": "{component.avatar.fontSize.xl.default} * 1.5",
              "$type": "fontSizes"
            },
            "inline": {
              "$value": "{component.avatar.fontSize.xl.symbol.default} - {component.avatar.config.inline.sizing.fontSize}",
              "$type": "fontSizes"
            }
          }
        }
      },
      "letterSpacing": {
        "default": {
          "$value": "2%",
          "$type": "letterSpacing"
        }
      },
      "textCase": {
        "default": {
          "$value": "{app.textCase.component.default}",
          "$type": "textCase"
        }
      },
      "config": {
        "default": {
          "sizing": {
            "ratio": {
              "$value": "1",
              "$type": "other",
              "description": "ratio defines the width of avatar. 1 = 1:1 / 1.5 = 3:2  / 2 = 2:1"
            },
            "step": {
              "$value": "0.5rem",
              "$type": "other"
            }
          },
          "radius": {
            "step": {
              "$value": "0.125rem",
              "$type": "other",
              "description": "(md/0.5) = cirlce\n0.125 = rounded"
            }
          }
        },
        "inline": {
          "sizing": {
            "step": {
              "$value": "0.5rem",
              "$type": "other"
            },
            "fontSize": {
              "$value": "0.25rem",
              "$type": "other"
            }
          }
        }
      }
    }
  }
}
