{
  "component": {
    "slider": {
      "typography": {
        "value": {
          "value": "{app.typography.control.number.default}",
          "type": "typography"
        },
        "label": {
          "value": "{app.typography.control.text.text}",
          "type": "typography"
        },
        "symbol": {
          "value": "{app.typography.control.number.default}",
          "type": "typography"
        }
      },
      "dimension": {
        "track": {
          "height": {
            "value": "{component.slider.config.track.height}",
            "type": "dimension"
          },
          "border": {
            "value": "{app.dimension.label.border.xs}",
            "type": "dimension"
          },
          "radius": {
            "value": "{component.slider.config.track.radius}",
            "type": "dimension"
          }
        },
        "container": {
          "height": {
            "value": "{component.slider.config.container.height}",
            "type": "dimension"
          },
          "padding": {
            "value": "{component.slider.dimension.handle.height} * 0.5",
            "type": "dimension"
          },
          "gap": {
            "value": "{component.slider.dimension.handle.height} * 0.5",
            "type": "dimension"
          }
        },
        "handle": {
          "height": {
            "value": "{component.slider.config.handle.height}",
            "type": "dimension"
          },
          "width": {
            "value": "{component.slider.config.handle.height} * {component.slider.config.handle.ratio}",
            "type": "dimension"
          },
          "border": {
            "value": "{app.dimension.label.border.xs}",
            "type": "dimension"
          },
          "radius": {
            "value": "{component.slider.config.handle.radius}",
            "type": "dimension"
          },
          "pressed": {
            "height": {
              "value": "{component.slider.dimension.handle.height} * {component.slider.config.handle.pressed}",
              "type": "dimension"
            },
            "width": {
              "value": "{component.slider.dimension.handle.width} * {component.slider.config.handle.pressed}",
              "type": "dimension"
            }
          }
        },
        "step": {
          "height": {
            "value": "{component.slider.config.track.height} - {component.slider.config.step.padding} * 2",
            "type": "dimension"
          },
          "width": {
            "value": "{component.slider.dimension.step.height} * {component.slider.config.step.ratio}",
            "type": "dimension"
          },
          "radius": {
            "value": "{component.slider.config.track.radius} - {component.slider.config.step.padding}",
            "type": "dimension"
          }
        }
      },
      "config": {
        "track": {
          "height": {
            "value": "0.25rem",
            "type": "other",
            "description": "-0.5 for material design style (handle is offset)"
          },
          "radius": {
            "value": "0.25rem",
            "type": "other",
            "description": "for 100% roundness make equal to height md"
          }
        },
        "handle": {
          "height": {
            "value": "1rem",
            "type": "other"
          },
          "radius": {
            "value": "1rem",
            "type": "other",
            "description": "for 100% roundness make half of pressed height"
          },
          "ratio": {
            "value": "1",
            "type": "other"
          },
          "pressed": {
            "value": "1.125",
            "type": "other"
          }
        },
        "label": {
          "symbol": {
            "default": {
              "value": "{app.fontSize.control.text.md}",
              "type": "other"
            },
            "step": {
              "value": "0.125rem",
              "type": "other"
            }
          },
          "text": {
            "default": {
              "value": "{app.fontSize.control.text.md}",
              "type": "other"
            },
            "step": {
              "value": "0.125rem",
              "type": "other"
            }
          }
        },
        "step": {
          "padding": {
            "value": "0.0625rem",
            "type": "other"
          },
          "ratio": {
            "value": "1",
            "type": "other"
          }
        },
        "container": {
          "height": {
            "value": "{app.dimension.control.height.xs}",
            "type": "other"
          }
        }
      },
      "color": {
        "default": {
          "track": {
            "bg": {
              "active": {
                "value": "{app.color.component.primary.fg.solid}",
                "type": "color"
              },
              "inactive": {
                "value": "{app.color.component.primary.border.translucent}",
                "type": "color"
              }
            },
            "border": {
              "default": {
                "value": "transparent",
                "type": "color"
              }
            }
          },
          "handle": {
            "bg": {
              "default": {
                "value": "{app.color.component.primary.fg.solid}",
                "type": "color"
              },
              "active": {
                "value": "{app.color.component.primary.fg.solid}",
                "type": "color",
                "$extensions": {
                  "studio.tokens": {
                    "modify": {
                      "type": "lighten",
                      "value": "{config.color.grey.lighten.default}",
                      "space": "hsl"
                    }
                  }
                }
              }
            },
            "border": {
              "default": {
                "value": "transparent",
                "type": "color"
              },
              "focus": {
                "value": "transparent",
                "type": "color"
              }
            }
          },
          "step": {
            "active": {
              "value": "{app.color.surface.elevation.base.bg.default}",
              "type": "color",
              "$extensions": {
                "studio.tokens": {
                  "modify": {
                    "type": "alpha",
                    "value": "0.4",
                    "space": "hsl"
                  }
                }
              }
            },
            "inactive": {
              "value": "{app.color.surface.fg.default}",
              "type": "color",
              "$extensions": {
                "studio.tokens": {
                  "modify": {
                    "type": "alpha",
                    "value": "0.2",
                    "space": "hsl"
                  }
                }
              }
            },
            "hidden": {
              "value": "transparent",
              "type": "color"
            }
          },
          "label": {
            "fg": {
              "default": {
                "value": "{app.color.component.primary.fg.solid}",
                "type": "color"
              }
            }
          }
        },
        "disabled": {
          "track": {
            "bg": {
              "active": {
                "value": "{app.color.component.disabled.fg.solid}",
                "type": "color"
              },
              "inactive": {
                "value": "{app.color.component.disabled.border.translucent}",
                "type": "color"
              }
            },
            "border": {
              "default": {
                "value": "transparent",
                "type": "color"
              }
            }
          },
          "handle": {
            "bg": {
              "default": {
                "value": "{app.color.component.disabled.fg.solid}",
                "type": "color"
              }
            },
            "border": {
              "default": {
                "value": "transparent",
                "type": "color"
              }
            }
          },
          "step": {
            "active": {
              "value": "{app.color.surface.elevation.base.bg.default}",
              "type": "color"
            },
            "inactive": {
              "value": "{app.color.component.disabled.fg.solid}",
              "type": "color"
            },
            "hidden": {
              "value": "transparent",
              "type": "color"
            }
          },
          "label": {
            "fg": {
              "default": {
                "value": "{app.color.component.disabled.fg.solid}",
                "type": "color"
              }
            }
          }
        }
      },
      "boxShadow": {
        "focusRing": {
          "value": "{app.boxShadow.focusRing.default.lg}",
          "type": "boxShadow"
        }
      }
    }
  }
}