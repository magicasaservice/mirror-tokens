{
  "component": {
    "slider": {
      "fontFamily": {
        "default": {
          "$value": "{app.fontFamily.component.text}",
          "$type": "fontFamilies"
        },
        "number": {
          "$value": "{app.fontFamily.component.number}",
          "$type": "fontFamilies"
        }
      },
      "fontWeight": {
        "label": {
          "$value": "{app.fontWeight.component.text.default}",
          "$type": "fontWeights"
        },
        "number": {
          "$value": "{app.fontWeight.component.number.default}",
          "$type": "fontWeights"
        }
      },
      "lineHeight": {
        "label": {
          "$value": "{app.lineHeight.default}",
          "$type": "lineHeights"
        },
        "number": {
          "$value": "{app.lineHeight.default}",
          "$type": "lineHeights"
        }
      },
      "dimension": {
        "track": {
          "height": {
            "$value": "{app.dimension.component.spacing.1}",
            "$type": "dimension"
          },
          "border": {
            "$value": "{app.dimension.component.border.thin}",
            "$type": "dimension"
          },
          "radius": {
            "$value": "{app.dimension.component.radius.round}",
            "$type": "dimension"
          }
        },
        "container": {
          "height": {
            "$value": "{app.dimension.component.spacing.8}",
            "$type": "dimension"
          },
          "padding": {
            "$value": "{component.slider.dimension.handle.height} / 2",
            "$type": "dimension"
          },
          "gap": {
            "$value": "{component.slider.dimension.handle.height} / 2",
            "$type": "dimension"
          }
        },
        "handle": {
          "height": {
            "$value": "{app.dimension.component.spacing.3_75}",
            "$type": "dimension"
          },
          "width": {
            "$value": "{component.slider.dimension.handle.height}",
            "$type": "dimension"
          },
          "border": {
            "$value": "{app.dimension.component.border.thin}",
            "$type": "dimension"
          },
          "outline": {
            "focus": {
              "$value": "{app.dimension.component.outline.focus.default}",
              "$type": "dimension"
            }
          },
          "radius": {
            "$value": "{app.dimension.component.radius.default.lg}",
            "$type": "dimension"
          },
          "pressed": {
            "height": {
              "$value": "{app.dimension.component.spacing.4_25}",
              "$type": "dimension"
            },
            "width": {
              "$value": "{app.dimension.component.spacing.4_25}",
              "$type": "dimension"
            }
          }
        },
        "step": {
          "height": {
            "$value": "{component.slider.dimension.track.height} - {app.dimension.component.spacing.0_25}",
            "$type": "dimension"
          },
          "width": {
            "$value": "{component.slider.dimension.step.height}",
            "$type": "dimension"
          },
          "radius": {
            "$value": "{app.dimension.component.spacing.4}",
            "$type": "dimension"
          }
        },
        "target": {
          "default": {
            "$value": "{component.slider.dimension.container.height} + {component.slider.dimension.container.padding}",
            "$type": "dimension"
          }
        },
        "symbol": {
          "default": {
            "$value": "{app.fontSize.component.text.sm}",
            "$type": "dimension"
          }
        }
      },
      "color": {
        "default": {
          "track": {
            "bg": {
              "active": {
                "$value": "{app.color.component.primary.fg.solid.default}",
                "$type": "color"
              },
              "inactive": {
                "$value": "{app.color.component.primary.bg.translucent.default}",
                "$type": "color"
              }
            },
            "border": {
              "default": {
                "$value": "transparent",
                "$type": "color"
              }
            }
          },
          "handle": {
            "bg": {
              "default": {
                "$value": "{app.color.component.primary.fg.solid.default}",
                "$type": "color"
              }
            },
            "border": {
              "default": {
                "$value": "transparent",
                "$type": "color"
              },
              "focus": {
                "$value": "transparent",
                "$type": "color"
              }
            },
            "outline": {
              "focus": {
                "$value": "{app.color.component.focus.outline.default}",
                "$type": "color"
              }
            }
          },
          "step": {
            "active": {
              "$value": "{app.color.component.primary.fg.onSolid.default}",
              "$type": "color"
            },
            "inactive": {
              "$value": "{app.color.component.primary.fg.subtle.default}",
              "$type": "color"
            },
            "hidden": {
              "$value": "transparent",
              "$type": "color"
            }
          },
          "label": {
            "fg": {
              "default": {
                "$value": "{app.color.component.primary.fg.solid.default}",
                "$type": "color"
              }
            }
          }
        },
        "disabled": {
          "track": {
            "bg": {
              "active": {
                "$value": "{app.color.component.disabled.fg.solid.default}",
                "$type": "color"
              },
              "inactive": {
                "$value": "{app.color.component.disabled.bg.translucent.default}",
                "$type": "color"
              }
            },
            "border": {
              "default": {
                "$value": "transparent",
                "$type": "color"
              }
            }
          },
          "handle": {
            "bg": {
              "default": {
                "$value": "{app.color.component.disabled.fg.solid.default}",
                "$type": "color"
              }
            },
            "border": {
              "default": {
                "$value": "transparent",
                "$type": "color"
              }
            }
          },
          "step": {
            "active": {
              "$value": "{app.color.component.disabled.fg.onSolid.default}",
              "$type": "color"
            },
            "inactive": {
              "$value": "{app.color.component.disabled.fg.subtle.default}",
              "$type": "color"
            },
            "hidden": {
              "$value": "transparent",
              "$type": "color"
            }
          },
          "label": {
            "fg": {
              "default": {
                "$value": "{app.color.component.disabled.fg.solid.default}",
                "$type": "color"
              }
            }
          }
        }
      },
      "fontSize": {
        "label": {
          "$value": "{app.fontSize.component.text.lg}",
          "$type": "fontSizes"
        },
        "number": {
          "$value": "{app.fontSize.component.text.sm}",
          "$type": "fontSizes"
        }
      },
      "letterSpacing": {
        "label": {
          "$value": "{app.letterSpacing.component.default}",
          "$type": "letterSpacing"
        },
        "number": {
          "$value": "{app.letterSpacing.component.number}",
          "$type": "letterSpacing"
        }
      }
    }
  }
}
