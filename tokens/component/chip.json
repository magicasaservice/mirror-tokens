{
  "component": {
    "chip": {
      "fontFamily": {
        "default": {
          "$type": "fontFamilies",
          "$value": "{app.fontFamily.control.text}"
        },
        "symbol": {
          "$type": "fontFamilies",
          "$value": "{app.fontFamily.control.symbol}"
        }
      },
      "fontWeight": {
        "default": {
          "$type": "fontWeights",
          "$value": "{app.fontWeight.control.text.strong}"
        },
        "symbol": {
          "$type": "fontWeights",
          "$value": "{app.fontWeight.control.symbol.default}"
        },
        "icon": {
          "$type": "fontWeights",
          "$value": "{app.fontWeight.control.symbol.alt}"
        }
      },
      "lineHeight": {
        "default": {
          "$type": "lineHeights",
          "$value": "{app.lineHeight.default}"
        },
        "symbol": {
          "$type": "lineHeights",
          "$value": "{app.lineHeight.none}"
        }
      },
      "fontSize": {
        "default": {
          "sm": {
            "$type": "fontSizes",
            "$value": "{component.chip.fontSize.default.md} - {component.chip.config.label.step}"
          },
          "md": {
            "$type": "fontSizes",
            "$value": "{app.fontSize.label.text.lg}"
          },
          "lg": {
            "$type": "fontSizes",
            "$value": "{component.chip.fontSize.default.md} + {component.chip.config.label.step}"
          }
        },
        "symbol": {
          "sm": {
            "$type": "fontSizes",
            "$value": "{component.chip.fontSize.symbol.md} - {component.chip.config.symbol.step}"
          },
          "md": {
            "$type": "fontSizes",
            "$value": "{app.fontSize.control.symbol.xs}"
          },
          "lg": {
            "$type": "fontSizes",
            "$value": "{component.chip.fontSize.symbol.md} + {component.chip.config.symbol.step}"
          }
        }
      },
      "icon": {
        "sm": {
          "$type": "fontSizes",
          "$value": "{component.chip.fontSize.symbol.sm} + {component.chip.config.icon.step}"
        },
        "md": {
          "$type": "fontSizes",
          "$value": "{component.chip.fontSize.symbol.md} + {component.chip.config.icon.step}"
        },
        "lg": {
          "$type": "fontSizes",
          "$value": "{component.chip.fontSize.symbol.lg} + {component.chip.config.icon.step}"
        }
      },
      "letterSpacing": {
        "default": {
          "$type": "letterSpacing",
          "$value": "{app.letterSpacing.control.text}"
        }
      },
      "textCase": {
        "default": {
          "$type": "textCase",
          "$value": "{app.textCase.control.default}"
        }
      },
      "color": {
        "default": {
          "solid": {
            "bg": {
              "default": {
                "$type": "color",
                "$value": "{app.color.component.primary.bg.solid.default}"
              },
              "hover": {
                "$type": "color",
                "$value": "{app.color.component.primary.bg.solid.hover}"
              },
              "active": {
                "$type": "color",
                "$value": "{app.color.component.primary.bg.solid.active}"
              },
              "dragged": {
                "$type": "color",
                "$value": "{app.color.component.primary.bg.solid.active}"
              }
            },
            "fg": {
              "default": {
                "$type": "color",
                "$value": "{app.color.component.primary.fg.onSolid}"
              }
            }
          },
          "translucent": {
            "bg": {
              "default": {
                "$type": "color",
                "$value": "{app.color.component.primary.bg.translucent.default}"
              },
              "hover": {
                "$type": "color",
                "$value": "{app.color.component.primary.bg.translucent.hover}"
              },
              "active": {
                "$type": "color",
                "$value": "{app.color.component.primary.bg.translucent.active}"
              },
              "dragged": {
                "$type": "color",
                "$value": "{app.color.component.primary.bg.translucent.active}"
              }
            },
            "fg": {
              "default": {
                "$type": "color",
                "$value": "{app.color.component.primary.fg.onTranslucent}"
              }
            }
          },
          "outline": {
            "bg": {
              "dragged": {
                "$type": "color",
                "$value": "transparent"
              }
            },
            "border": {
              "default": {
                "$type": "color",
                "$value": "{app.color.component.primary.border.translucent.default}"
              },
              "hover": {
                "$type": "color",
                "$value": "{app.color.component.primary.border.translucent.hover}"
              },
              "active": {
                "$type": "color",
                "$value": "{app.color.component.primary.border.translucent.active}"
              },
              "dragged": {
                "$type": "color",
                "$value": "app.color.component.primary.border.translucent.active"
              }
            },
            "fg": {
              "default": {
                "$type": "color",
                "$value": "{app.color.component.primary.fg.solid.default}"
              }
            }
          },
          "tone": {
            "bg": {
              "default": {
                "$type": "color",
                "$value": "{app.color.component.primary.bg.tone.default}"
              },
              "hover": {
                "$type": "color",
                "$value": "{app.color.component.primary.bg.tone.hover}"
              },
              "active": {
                "$type": "color",
                "$value": "{app.color.component.primary.bg.tone.active}"
              },
              "dragged": {
                "$type": "color",
                "$value": "{app.color.component.primary.bg.tone.active}"
              }
            },
            "fg": {
              "default": {
                "$type": "color",
                "$value": "{app.color.component.primary.fg.onTone}"
              }
            }
          }
        },
        "disabled": {
          "solid": {
            "bg": {
              "default": {
                "$type": "color",
                "$value": "{app.color.component.disabled.bg.solid}"
              }
            },
            "fg": {
              "default": {
                "$type": "color",
                "$value": "{app.color.component.disabled.fg.solid}"
              }
            }
          },
          "translucent": {
            "bg": {
              "default": {
                "$type": "color",
                "$value": "{app.color.component.disabled.bg.translucent}"
              }
            },
            "fg": {
              "default": {
                "$type": "color",
                "$value": "{app.color.component.disabled.fg.solid}"
              }
            }
          },
          "outline": {
            "border": {
              "default": {
                "$type": "color",
                "$value": "{app.color.component.disabled.border.translucent}"
              }
            },
            "fg": {
              "default": {
                "$type": "color",
                "$value": "{app.color.component.disabled.fg.solid}"
              }
            }
          },
          "tone": {
            "bg": {
              "default": {
                "$type": "color",
                "$value": "{app.color.component.disabled.bg.tone}"
              }
            },
            "fg": {
              "default": {
                "$type": "color",
                "$value": "{app.color.component.disabled.fg.onTone}"
              }
            }
          }
        },
        "focus": {
          "border": {
            "default": {
              "$type": "color",
              "$value": "{app.color.component.focus.border.default}"
            }
          }
        }
      },
      "dimension": {
        "height": {
          "sm": {
            "$type": "dimension",
            "$value": "{app.dimension.control.height.xxs}"
          },
          "md": {
            "$type": "dimension",
            "$value": "{app.dimension.control.height.xs}"
          },
          "lg": {
            "$type": "dimension",
            "$value": "{app.dimension.control.height.sm}"
          }
        },
        "padding": {
          "sm": {
            "$type": "dimension",
            "$value": "{app.dimension.control.padding.xxs}"
          },
          "md": {
            "$type": "dimension",
            "$value": "{app.dimension.control.padding.xs}"
          },
          "lg": {
            "$type": "dimension",
            "$value": "{app.dimension.control.padding.sm}"
          },
          "label": {
            "$type": "dimension",
            "$value": "4px"
          }
        },
        "gap": {
          "sm": {
            "$type": "dimension",
            "$value": "{app.dimension.control.gap.xxs}"
          },
          "md": {
            "$type": "dimension",
            "$value": "{app.dimension.control.gap.xs}"
          },
          "lg": {
            "$type": "dimension",
            "$value": "{app.dimension.control.gap.sm}"
          }
        },
        "radius": {
          "default": {
            "sm": {
              "$type": "dimension",
              "$value": "{app.dimension.control.radius.xxs}"
            },
            "md": {
              "$type": "dimension",
              "$value": "{app.dimension.control.radius.xs}"
            },
            "lg": {
              "$type": "dimension",
              "$value": "{app.dimension.control.radius.sm}"
            }
          },
          "inline": {
            "sm": {
              "$type": "dimension",
              "$value": "{component.chip.dimension.radius.default.sm} - {component.chip.config.dimension.radius.inline}"
            },
            "md": {
              "$type": "dimension",
              "$value": "{component.chip.dimension.radius.default.md} - {component.chip.config.dimension.radius.inline}"
            },
            "lg": {
              "$type": "dimension",
              "$value": "{component.chip.dimension.radius.default.lg} - {component.chip.config.dimension.radius.inline}"
            }
          }
        },
        "border": {
          "default": {
            "$type": "dimension",
            "$value": "{app.dimension.label.border.default}"
          }
        },
        "symbol": {
          "sm": {
            "default": {
              "$type": "dimension",
              "$value": "{component.chip.fontSize.symbol.sm}"
            },
            "icon": {
              "$type": "dimension",
              "$value": "{component.chip.dimension.symbol.sm.default} + {component.chip.config.symbol.icon.step}"
            }
          },
          "md": {
            "default": {
              "$type": "dimension",
              "$value": "{component.chip.fontSize.symbol.md}"
            },
            "icon": {
              "$type": "dimension",
              "$value": "{component.chip.dimension.symbol.md.default} + {component.chip.config.symbol.icon.step}"
            }
          },
          "lg": {
            "default": {
              "$type": "dimension",
              "$value": "{component.chip.fontSize.symbol.lg}"
            },
            "icon": {
              "$type": "dimension",
              "$value": "{component.chip.dimension.symbol.lg.default} + {component.chip.config.symbol.icon.step}"
            }
          }
        },
        "blur": {
          "default": {
            "$type": "dimension",
            "$value": "4rem"
          }
        }
      },
      "config": {
        "icon": {
          "step": {
            "$type": "other",
            "$value": "0.25rem"
          }
        },
        "symbol": {
          "step": {
            "$type": "other",
            "$value": "0.125rem"
          }
        },
        "label": {
          "step": {
            "$type": "other",
            "$value": "0.0625rem"
          }
        },
        "dimension": {
          "radius": {
            "inline": {
              "$type": "other",
              "$value": "{component.chip.dimension.radius.default.md} - {component.chip.dimension.radius.default.sm}"
            }
          }
        }
      },
      "boxShadow": {
        "focusRing": {
          "$type": "boxShadow",
          "$value": "{app.boxShadow.focusRing.md}"
        },
        "dragged": {
          "$type": "boxShadow",
          "$value": [
            {
              "x": "0",
              "y": "0.375rem",
              "blur": "1rem",
              "spread": "0",
              "color": "{app.color.surface.shadow.strong}",
              "type": "dropShadow"
            }
          ]
        }
      }
    }
  }
}