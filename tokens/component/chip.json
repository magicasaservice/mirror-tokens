{
  "component": {
    "chip": {
      "fontFamily": {
        "default": {
          "$value": "{app.fontFamily.component.text}",
          "$type": "fontFamilies"
        },
        "symbol": {
          "$value": "{app.fontFamily.component.symbol}",
          "$type": "fontFamilies"
        }
      },
      "fontWeight": {
        "default": {
          "$value": "{app.fontWeight.component.text.strong}",
          "$type": "fontWeights"
        },
        "symbol": {
          "$value": "{app.fontWeight.component.symbol.default}",
          "$type": "fontWeights"
        },
        "icon": {
          "$value": "{app.fontWeight.component.symbol.alt}",
          "$type": "fontWeights"
        }
      },
      "lineHeight": {
        "default": {
          "$value": "{app.lineHeight.default}",
          "$type": "lineHeights"
        },
        "symbol": {
          "$value": "{app.lineHeight.none}",
          "$type": "lineHeights"
        }
      },
      "fontSize": {
        "default": {
          "sm": {
            "$value": "{component.chip.fontSize.default.md} - 0.0625rem",
            "$type": "fontSizes"
          },
          "md": {
            "$value": "{app.fontSize.component.text.sm}",
            "$type": "fontSizes"
          },
          "lg": {
            "$value": "{component.chip.fontSize.default.md} + 0.0625rem",
            "$type": "fontSizes"
          }
        },
        "symbol": {
          "sm": {
            "$value": "{component.chip.fontSize.symbol.md} - 0.125rem",
            "$type": "fontSizes"
          },
          "md": {
            "$value": "{app.fontSize.component.symbol.lg}",
            "$type": "fontSizes"
          },
          "lg": {
            "$value": "{component.chip.fontSize.symbol.md} + 0.125rem",
            "$type": "fontSizes"
          }
        }
      },
      "icon": {
        "sm": {
          "$value": "{component.chip.fontSize.symbol.sm} + 0.25rem",
          "$type": "fontSizes"
        },
        "md": {
          "$value": "{component.chip.fontSize.symbol.md} + 0.25rem",
          "$type": "fontSizes"
        },
        "lg": {
          "$value": "{component.chip.fontSize.symbol.lg} + 0.25rem",
          "$type": "fontSizes"
        }
      },
      "letterSpacing": {
        "default": {
          "$value": "{app.letterSpacing.component.default}",
          "$type": "letterSpacing"
        }
      },
      "textCase": {
        "default": {
          "$value": "{app.textCase.component.default}",
          "$type": "textCase"
        }
      },
      "color": {
        "default": {
          "solid": {
            "bg": {
              "default": {
                "$value": "{app.color.component.primary.bg.solid.default}",
                "$type": "color"
              },
              "hover": {
                "$value": "{app.color.component.primary.bg.solid.hover}",
                "$type": "color"
              },
              "active": {
                "$value": "{app.color.component.primary.bg.solid.active}",
                "$type": "color"
              },
              "dragged": {
                "$value": "{app.color.component.primary.bg.solid.active}",
                "$type": "color"
              }
            },
            "fg": {
              "default": {
                "$value": "{app.color.component.primary.fg.onSolid.default}",
                "$type": "color"
              }
            }
          },
          "translucent": {
            "bg": {
              "default": {
                "$value": "{app.color.component.primary.bg.translucent.default}",
                "$type": "color"
              },
              "hover": {
                "$value": "{app.color.component.primary.bg.translucent.hover}",
                "$type": "color"
              },
              "active": {
                "$value": "{app.color.component.primary.bg.translucent.active}",
                "$type": "color"
              },
              "dragged": {
                "$value": "{app.color.component.primary.bg.translucent.active}",
                "$type": "color"
              }
            },
            "fg": {
              "default": {
                "$value": "{app.color.component.primary.fg.onTranslucent.default}",
                "$type": "color"
              }
            }
          },
          "outline": {
            "bg": {
              "dragged": {
                "$value": "transparent",
                "$type": "color"
              }
            },
            "border": {
              "default": {
                "$value": "{app.color.component.primary.border.translucent.default}",
                "$type": "color"
              },
              "hover": {
                "$value": "{app.color.component.primary.border.translucent.hover}",
                "$type": "color"
              },
              "active": {
                "$value": "{app.color.component.primary.border.translucent.active}",
                "$type": "color"
              },
              "dragged": {
                "$value": "app.color.component.primary.border.translucent.active",
                "$type": "color"
              }
            },
            "fg": {
              "default": {
                "$value": "{app.color.component.primary.fg.solid.default}",
                "$type": "color"
              }
            }
          },
          "tone": {
            "bg": {
              "default": {
                "$value": "{app.color.component.primary.bg.tone.default}",
                "$type": "color"
              },
              "hover": {
                "$value": "{app.color.component.primary.bg.tone.hover}",
                "$type": "color"
              },
              "active": {
                "$value": "{app.color.component.primary.bg.tone.active}",
                "$type": "color"
              },
              "dragged": {
                "$value": "{app.color.component.primary.bg.tone.active}",
                "$type": "color"
              }
            },
            "fg": {
              "default": {
                "$value": "{app.color.component.primary.fg.onTone.default}",
                "$type": "color"
              }
            }
          }
        },
        "disabled": {
          "solid": {
            "bg": {
              "default": {
                "$value": "{app.color.component.disabled.bg.solid.default}",
                "$type": "color"
              }
            },
            "fg": {
              "default": {
                "$value": "{app.color.component.disabled.fg.solid.default}",
                "$type": "color"
              }
            }
          },
          "translucent": {
            "bg": {
              "default": {
                "$value": "{app.color.component.disabled.bg.translucent.default}",
                "$type": "color"
              }
            },
            "fg": {
              "default": {
                "$value": "{app.color.component.disabled.fg.solid.default}",
                "$type": "color"
              }
            }
          },
          "outline": {
            "border": {
              "default": {
                "$value": "{app.color.component.disabled.border.translucent.default}",
                "$type": "color"
              }
            },
            "fg": {
              "default": {
                "$value": "{app.color.component.disabled.fg.solid.default}",
                "$type": "color"
              }
            }
          },
          "tone": {
            "bg": {
              "default": {
                "$value": "{app.color.component.disabled.bg.tone.default}",
                "$type": "color"
              }
            },
            "fg": {
              "default": {
                "$value": "{app.color.component.disabled.fg.onTone.default}",
                "$type": "color"
              }
            }
          }
        },
        "focus": {
          "border": {
            "default": {
              "$value": "{app.color.component.focus.border.default}",
              "$type": "color"
            }
          },
          "outline": {
            "default": {
              "$value": "{app.color.component.focus.outline.default}",
              "$type": "color"
            }
          }
        }
      },
      "dimension": {
        "height": {
          "sm": {
            "$value": "{app.dimension.component.height.sm}",
            "$type": "dimension"
          },
          "md": {
            "$value": "{app.dimension.component.height.md}",
            "$type": "dimension"
          },
          "lg": {
            "$value": "{app.dimension.component.height.lg}",
            "$type": "dimension"
          }
        },
        "padding": {
          "sm": {
            "$value": "{app.dimension.component.padding.2xs}",
            "$type": "dimension"
          },
          "md": {
            "$value": "{app.dimension.component.padding.xs}",
            "$type": "dimension"
          },
          "lg": {
            "$value": "{app.dimension.component.padding.sm}",
            "$type": "dimension"
          },
          "label": {
            "$value": "4px",
            "$type": "dimension"
          }
        },
        "gap": {
          "sm": {
            "$value": "{app.dimension.component.gap.2xs}",
            "$type": "dimension"
          },
          "md": {
            "$value": "{app.dimension.component.gap.xs}",
            "$type": "dimension"
          },
          "lg": {
            "$value": "{app.dimension.component.gap.sm}",
            "$type": "dimension"
          }
        },
        "radius": {
          "default": {
            "sm": {
              "$value": "{app.dimension.component.radius.2xs}",
              "$type": "dimension"
            },
            "md": {
              "$value": "{app.dimension.component.radius.xs}",
              "$type": "dimension"
            },
            "lg": {
              "$value": "{app.dimension.component.radius.sm}",
              "$type": "dimension"
            }
          },
          "inline": {
            "sm": {
              "$value": "{component.chip.dimension.radius.default.sm} - {component.chip.dimension.radius.default.md} - {component.chip.dimension.radius.default.sm}",
              "$type": "dimension"
            },
            "md": {
              "$value": "{component.chip.dimension.radius.default.md} - {component.chip.dimension.radius.default.md} - {component.chip.dimension.radius.default.sm}",
              "$type": "dimension"
            },
            "lg": {
              "$value": "{component.chip.dimension.radius.default.lg} - {component.chip.dimension.radius.default.md} - {component.chip.dimension.radius.default.sm}",
              "$type": "dimension"
            }
          }
        },
        "border": {
          "default": {
            "$value": "{app.dimension.label.border.default}",
            "$type": "dimension"
          }
        },
        "outline": {
          "focus": {
            "$value": "{app.dimension.component.outline.focus.md}"
          }
        },
        "symbol": {
          "sm": {
            "default": {
              "$value": "{component.chip.fontSize.symbol.sm}",
              "$type": "dimension"
            },
            "icon": {
              "$value": "{component.chip.dimension.symbol.sm.default} + {component.chip.config.symbol.icon.step}",
              "$type": "dimension"
            }
          },
          "md": {
            "default": {
              "$value": "{component.chip.fontSize.symbol.md}",
              "$type": "dimension"
            },
            "icon": {
              "$value": "{component.chip.dimension.symbol.md.default} + {component.chip.config.symbol.icon.step}",
              "$type": "dimension"
            }
          },
          "lg": {
            "default": {
              "$value": "{component.chip.fontSize.symbol.lg}",
              "$type": "dimension"
            },
            "icon": {
              "$value": "{component.chip.dimension.symbol.lg.default} + {component.chip.config.symbol.icon.step}",
              "$type": "dimension"
            }
          }
        },
        "blur": {
          "default": {
            "$value": "4rem",
            "$type": "dimension"
          }
        }
      },
      "boxShadow": {
        "dragged": {
          "$value": [
            {
              "x": "0",
              "y": "0.375rem",
              "blur": "1rem",
              "spread": "0",
              "color": "{app.color.surface.shadow.strong}",
              "type": "dropShadow"
            }
          ],
          "$type": "boxShadow"
        }
      }
    }
  }
}
